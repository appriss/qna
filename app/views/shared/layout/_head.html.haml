%title
  &= page_title
= render "shared/layout/css"

= javascript_include_tag "modernizr.custom.geo.load.js"

/[if lt IE 9]
  <script src="https://html5shim.googlecode.com/svn/trunk/html5.js"></script>

= raw(current_group.head_tag)
= render "shared/layout/meta"

-if @feed_urls && !@feed_urls.empty?
  -@feed_urls.each do |title, url|
    %link{:rel=>"alternate", :type=>"application/atom+xml", :title => title, :href=> url}

= render "shared/analytics"

